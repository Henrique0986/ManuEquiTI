@model ManuEquiTI.Models.InvtSoft

@{
    ViewData["Title"] = "Deletar";
}

<h1>Deletar Software</h1>

<h3>Tem certeza que deseja deletar esse Software?</h3>
<div>
    <h4>InvtSoft</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.SoftWare)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SoftWare)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CodSoft)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodSoft)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Observacoes)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Observacoes)
        </dd>
    </dl>
    
    <form asp-action="Deletar">
        <input type="hidden" asp-for="idSoft" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Voltar</a>
    </form>
</div>
