@model IEnumerable<ManuEquiTI.Models.InvtSoft>

@{
    ViewData["Title"] = "Inventário de SoftWares";
}

<h1>Inventário de SoftWares</h1>

<p>
    <a asp-action="Create">Criar um novo</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SoftWare)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CodSoft)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Observacoes)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SoftWare)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CodSoft)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observacoes)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.idSoft">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.idSoft">Detalhes</a> |
                <a asp-action="Delete" asp-route-id="@item.idSoft">Deletar</a>
            </td>
        </tr>
}
    </tbody>
</table>
