@model ManuEquiTI.Models.InvtMaq

@{
    ViewData["Title"] = "Deletar";
}

<h1>Deletar Maquinas</h1>

<h3>Tem certeza que deseja deletar essa Maquina?</h3>
<div>
    <h4>InvtMaq</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Maquina)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Maquina)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CodMaq)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodMaq)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Observacoes)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Observacoes)
        </dd>
    </dl>
    
    <form asp-action="Deletar">
        <input type="hidden" asp-for="idMaq" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-action="Index">Voltar</a>
    </form>
</div>
